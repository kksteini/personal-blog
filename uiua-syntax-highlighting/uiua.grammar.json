    {
      "name": "iterating-modifier",
      "match": "[⍥/∩]"
    },
    {
      "name": "dyadic-pervasive",
      "match": "[+]"
    },
    {
      "name": "dyadic-function",
      "match": "[-÷ⁿ×↯⊟⊂⊏↘]"
    },
    {
      "name": "dyadic-modifier",
      "match": "[⍜⊃]"
    },
    {
      "name": "bound-function",
      "match": "[FG]"
